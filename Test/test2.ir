FUNCTION fact :
PARAM n
iamt0 := n
iamt1 := #1
IF iamt0  == iamt1 GOTO iamlabel0
GOTO iamlabel1
LABEL iamlabel0 :
iamt2 := n
RETURN iamt2
GOTO iamlabel2
LABEL iamlabel1 :
iamt5 := n - #1
ARG iamt5
iamt4 := CALL fact
iamt3 :=  n * iamt4
RETURN iamt3
LABEL iamlabel2 :

FUNCTION main :
READ m
m := m + #2
m := m - #2
iamt6 := m
iamt7 := #1
IF iamt6  > iamt7 GOTO iamlabel3
GOTO iamlabel4
LABEL iamlabel3 :
iamt8 := m
ARG iamt8
result := CALL fact
GOTO iamlabel5
LABEL iamlabel4 :
result := #1
LABEL iamlabel5 :
iamt9 := result
WRITE iamt9
iamt10 := #0
RETURN iamt10

