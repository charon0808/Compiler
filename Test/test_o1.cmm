struct Operands
{
    int o1;
    int o2;
};

int add(struct Operands temp)
{
    return (temp.o1 + temp.o2);
}

int main()
{
    int n;
    struct Operands op;
    int a, b;
    a = 100 + 3;
    b=10;
    op.o1 = a+b;
    op.o2 = 2;
    n = add(op);
    write(n);
    return 0;
}