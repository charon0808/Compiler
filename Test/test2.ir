FUNCTION fact :
PARAM n
IF n  == #1 GOTO iamlabel0
iamt2 := n - #1
ARG iamt2
iamt1 := CALL fact
iamt0 := n * iamt1
RETURN iamt0
GOTO iamlabel2
LABEL iamlabel0 :
RETURN n
LABEL iamlabel2 :

FUNCTION main :
READ m
IF m  > #1 GOTO iamlabel3
result := #1
GOTO iamlabel5
LABEL iamlabel3 :

ARG m
result := CALL fact
LABEL iamlabel5 :

WRITE result
RETURN #0

